{
	"_version": "1.58.0",
	"sap.app": {
		"id": "ui5.walkthrough",
		
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	
		
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://Fiori2/F0002"
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.108.0",
			"libs": {
				"sap.m": {},
				"sap.tnt": {},
				"sap.f": {},
				"sap.ui.core": {},
                "sap.ui.layout": {}
						
			}
		},
		"models": {
		

		},
	
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"handleValidation": true,
		"rootView": {
            "viewName": "ui5.walkthrough.view.App",
            "type": "XML",
			"id": "app",
			"async": true
   
        },
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},

	"routing": {
	  "config": {
		"routerClass": "sap.m.routing.Router",
		"type": "View",
		"viewType": "XML",
		"path": "ui5.walkthrough.view",
		"controlId": "app",
		"controlAggregation": "pages",
        "async": true
	  },
	  "routes": [
		{
			"pattern": "login",
			"name": "login",
			"target": "login"
		  },

		{
			"pattern": "home",
			"name": "home",
			"target": ["home", "employees"]
		  },
		  
		{
			"pattern": "users",
			"name": "users",
			"target": "users"
		},
		{
			"pattern": "dashboard",
			"name": "dashboard",
			"target": "dashboard"
			
		},
		{
            "pattern": "itemForm/{mode}/{item}",
            "name": "itemForm",
            "target": "itemForm"
        },
		{
			"pattern": "employee/{employeeId}",
			"name": "employeeProfile",
			"target": "employeeProfile"
		},
		{
			"pattern": "employees",
			"name": "employees",
			"target": "employees"
		}

	  ],
		"targets": {
			"login": {
				"viewName": "Login"
			  },

			"home": {
				"viewName": "Main",
				"viewType": "XML",
				"controlAggregation": "pages"
			
			  },

			  "employees": {
				"viewName": "employees.Employee",
				"controlId": "appContainer",
				"parent": "home",
				"viewLevel": 1
			},
			"users": {
				"viewName": "User",
				"controlId": "appContainer",
				"parent": "home",
				"viewLevel": 1
			},

		
			"employeeProfile": {
				"viewName": "employees.EmployeeProfile",
				"viewLevel": 1,
				"controlId": "appContainer",
				"parent": "home"
			},
		
			"itemForm": {
				"viewName": "items.Form",
				"controlId": "appContainer",
				"parent": "home",
				"viewLevel": 1
			}
		}
	}
}
}

