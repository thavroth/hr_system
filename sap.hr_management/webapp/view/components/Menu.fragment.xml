		xmlns:core="sap.ui.core">
	<Menu itemSelected="onMenuAction">
        <MenuItem text="My profile" icon="sap-icon://person-placeholder" key="employees" />
        <MenuItem text="Logout" icon="sap-icon://log" key="logout" press="onLogoutPress">
            <customData>
                <core:CustomData
                    key="target"
                    value="p1" />
            </customData>
        </MenuItem>
        <MenuItem text="Save" icon="sap-icon://save" press="onPress" />
        <MenuItem text="Open" icon="sap-icon://open-folder" press="onPress" />
	</Menu>
</core:FragmentDefinition>